.container{
  width:100%;
  height:100%;
  display: flex;
  box-sizing: border-box;
  justify-content: center;
  padding:20rpx 10rpx;
  overflow: auto;
}
page{
  background-color: #fff;
}

.img-container{
  display: flex;
  flex-wrap:wrap;
}

.img{
  width:230rpx;
  height:230rpx;
  padding:3rpx;
}

.item{
  width: 100%;
  box-sizing: border-box;
  padding: 0 20rpx;
  background-color: #ffffff;
}