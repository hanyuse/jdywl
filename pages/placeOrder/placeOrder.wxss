
 .position{
  width: 100%;
  overflow-x: hidden;
  height: 100%;
} 

.palce{
  display: flex;
  align-items: center;
}

.item{
  width:33.3%;
  flex: 0 1 auto;
  display: flex;
  justify-content: center;
  align-items: center;
}

.text{
  display: inline-block;
  margin-right: 20rpx;
}

.carType{
  display: flex;
  align-items: center;
  border: 1px solid #20B2AA;
  border-radius: 10rpx;
  color: #20B2AA;
}

.hasaddtioncarType{
  display: flex;
  align-items: center;
  border: 1px solid  rgb(141, 211, 207);
  border-radius: 10rpx;
  color:  rgb(141, 211, 207);
}

.car{
  display: inline-block;
  padding: 10rpx; 
  width: 120rpx;
  text-align: center;
  box-sizing: border-box; 
}

.supervise{
  display: inline-block;
  padding: 10rpx; 
  width: 180rpx;
  text-align: center;
  box-sizing: border-box; 
}


.choose{ 
  color: #fff;
  background-color: #20B2AA;
}

.flex{
  flex: 1 0 auto;
}

.remind{
  width: 48rpx;
  height: 48rpx;
}


.bottom{
  position: fixed;
  width: 100%;
  height: 110rpx;
  left: 0;
  bottom: 0;
  z-index: 9999;
}

.bill{
  width:80%;
  box-sizing: border-box;
  color: #fff;
  background-color: #666;
  padding: 10rpx 5rpx 10rpx 30rpx;
  height: 110rpx;
}

/* .bill-item{
  flex: 1 0 auto;
} */
.submit-view{
  width:20%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.submit{
  box-sizing: border-box;
  height: 110rpx;
  width:100%;
  text-align: center;
  color: #fff;
  background-color: #20B2AA;
  border-radius: 0;
}

.hover{
  background-color: rgb(85, 180, 176);
}

.chooseDisable{
  background-color: rgb(141, 211, 207);
}